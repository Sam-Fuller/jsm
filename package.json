{
	"name": "jsm",
	"displayName": "jsm",
	"description": "run javascript on a file",
	"version": "0.0.4",

	"publisher": "SamFuller",

	"engines": {
		"vscode": "^1.57.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:jsm.map",
		"onCommand:jsm.mapString",
		"onCommand:jsm.filter",
		"onCommand:jsm.filterRegex",
		"onCommand:jsm.all",
		"onCommand:jsm.generate"

	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "jsm.map",
				"title": "jsmap"
			},
			{
				"command": "jsm.mapString",
				"title": "jsstring"
			},
			{
				"command": "jsm.filter",
				"title": "jsfilter"
			},
			{
				"command": "jsm.filterRegex",
				"title": "jsregex"
			},
			{
				"command": "jsm.all",
				"title": "jsall"
			},
			{
				"command": "jsm.generate",
				"title": "jsgenerate"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"build": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map"
	},
	"devDependencies": {
		"@types/vscode": "^1.57.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2",
		"ts-loader": "^9.2.2",
		"webpack": "^5.38.1",
		"webpack-cli": "^4.7.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"eslint-plugin-import": "^2.22.1",
		"prettier": "^2.2.1",
		"prettier-eslint": "^12.0.0",
		"jest": "^26.6.2"
	}
}
